{
  "name": "fe-compiler",
  "version": "1.0.0",
  "description": "A Frontend Compiling Boilerplate with Gulp, Webpack 3, Babel, esLint, Stylelint, nGrok, PostCSS and more",
  "main": "package.json",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mads-thines/fe-compiler.git"
  },
  "keywords": [
    "mads-thines",
    "Frontend",
    "Compiling",
    "Boilerplate",
    "SCSS",
    "JavaScript",
    "Gulp",
    "Webpack 3",
    "Babel",
    "esLint",
    "Stylelint",
    "nGrok",
    "PostCSS"
  ],
  "author": "mads-thines",
  "bugs": {
    "url": "https://github.com/mads-thines/fe-compiler/issues"
  },
  "scripts": {
    "start": "npm-run-all --parallel browserSync watch:gulp watch:webpack",
    "watch:gulp": "gulp default",
    "watch:webpack": "webpack --config config/webpack.dev.js",
    "build:gulp": "gulp scss",
    "build:webpack": "webpack --config config/webpack.prod.js",
    "build:prod": "npm-run-all build:gulp build:webpack",
    "browserSync": "browser-sync start --proxy 'dev.tjm-forsikring' --files '**/*.twig, **/*.css, **/*.js'",
    "preinstall": "npm install -g browser-sync",
    "ngrok": "ngrok http -auth='adapt:test' -host-header=dev.tjm-forsikring 80"
  },
  "homepage": "https://github.com/mads-thines/fe-compiler#readme",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.32",
    "adapt-mixins": "2.0.1",
    "autoprefixer": "7.1.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-trailing-function-commas": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-imports": "1.4.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "browser-sync": "2.18.13",
    "cssnano": "3.10.0",
    "eslint": "4.11.0",
    "eslint-loader": "1.9.0",
    "file-loader": "1.1.5",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-babel": "7.0.0",
    "gulp-eslint": "4.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-postcss": "7.0.0",
    "gulp-rename": "1.2.2",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.6.1",
    "gulp-stylelint": "5.0.0",
    "gulp-stylelint-sourcemap": "3.4.2",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "lodash": "4.17.4",
    "ngrok": "2.2.24",
    "node-sass-tilde-importer": "1.0.0",
    "npm": "5.5.1",
    "npm-run-all": "4.1.2",
    "postcss-inline-svg": "3.0.0",
    "stylelint": "8.2.0",
    "stylelint-scss": "2.1.0",
    "uglifyjs-webpack-plugin": "1.1.1",
    "webpack": "3.8.1",
    "webpack-merge": "^4.1.1",
    "webpack-stream": "4.0.0"
  },
  "dependencies": {
    "normalize.css": "7.0.0"
  }
}
