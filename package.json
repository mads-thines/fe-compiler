{
  "name": "fe-compiler",
  "author": "mads-thines",
  "version": "1.0.0",
  "description": "A Frontend Compiling Boilerplate with Gulp, Webpack 3, Babel, esLint, Stylelint, nGrok, PostCSS and more",
  "main": "package.json",
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "npm-run-all --parallel watch:gulp watch:webpack browserSync",
    "watch": "npm-run-all --parallel watch:gulp watch:webpack",
    "watch:gulp": "gulp default",
    "watch:webpack": "webpack --mode development --config config/webpack.dev.js",
    "build": "npm-run-all build:gulp build:webpack",
    "build:gulp": "gulp scss",
    "build:webpack": "webpack --mode production --config config/webpack.prod.js",
    "browserSync": "browser-sync start --config 'config/browsersync.js'",
    "ngrok": "ngrok -auth='adapt:logmein' -host-header=localhost 80"
  },
  "keywords": [
    "Frontend",
    "Compiling",
    "Boilerplate",
    "SCSS",
    "JavaScript",
    "Gulp",
    "Webpack 3",
    "Babel",
    "esLint",
    "Stylelint",
    "nGrok",
    "PostCSS"
  ],
  "devDependencies": {
    "@babel/core": "7.0.0-beta.32",
    "adapt-mixins": "2.0.7",
    "autoprefixer": "8.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-trailing-function-commas": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-imports": "1.5.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "browser-sync": "2.23.6",
    "compression-webpack-plugin": "1.1.10",
    "cssnano": "4.0.0-rc.2",
    "esdoc": "1.0.4",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "4.18.2",
    "eslint-loader": "2.0.0",
    "file-loader": "1.1.11",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "5.0.0",
    "gulp-babel": "7.0.1",
    "gulp-eslint": "4.0.2",
    "gulp-gzip": "1.4.2",
    "gulp-plumber": "1.2.0",
    "gulp-postcss": "7.0.1",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-stylelint": "7.0.0",
    "gulp-stylelint-sourcemap": "3.4.2",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "lodash": "4.17.5",
    "ngrok": "2.2.24",
    "node-sass": "4.7.2",
    "node-sass-tilde-importer": "1.0.1",
    "npm": "5.7.1",
    "npm-run-all": "4.1.2",
    "postcss-inline-svg": "3.0.0",
    "stylelint": "9.1.1",
    "stylelint-scss": "2.4.0",
    "uglifyjs-webpack-plugin": "1.2.2",
    "webpack": "4.1.0",
    "webpack-cli": "2.0.10",
    "webpack-merge": "4.1.2",
    "webpack-stream": "4.0.2"
  },
  "dependencies": {
    "gsap": "1.20.4",
    "normalize.css": "8.0.0",
    "smooth-scroll": "12.1.5",
    "svg4everybody": "2.1.9"
  }
}
